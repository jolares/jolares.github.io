{
  "name": "jolares-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "npx docusaurus",
    "start": "npx docusaurus start",
    "build": "npx docusaurus build && rm -rf ../../dist/apps/jolares-web && mkdir -p ../../dist/apps/jolares-web && cp -R build/ ../../dist/apps/jolares-web",
    "swizzle": "npx docusaurus swizzle",
    "deploy": "npx docusaurus deploy",
    "clear": "npx docusaurus clear",
    "serve": "npx docusaurus serve",
    "write-translations": "npx docusaurus write-translations",
    "write-heading-ids": "npx docusaurus write-heading-ids"
  },
  "dependencies": {
    "@docusaurus/core": "2.4.0",
    "@docusaurus/preset-classic": "2.4.0",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.2.1",
    "docusaurus-plugin-sass": "^0.2.3",
    "prism-react-renderer": "^1.3.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.62.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.4.0"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [],
        "dependsOn": [
          "^build"
        ]
      }
    },
    "tags": [
      "type:app",
      "scope:jolares-web"
    ]
  }
}
